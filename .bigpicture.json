{
    "port": 44525,
    "ignore": [
        "web",
        "bin",
        "internal/browser/pyproject",
        "internal/browser/javaproject"
    ],
    "validators": [
        {
            "type": "no_import",
            "args": {
                "from": "/internal/config",
                "to": "*"
            }
        },
        {
            "type": "no_import",
            "args": {
                "from": "/internal/server",
                "to": "*"
            }
        },
        {
            "type": "line_count",
            "args": {
                "module": "/internal/browser",
                "max": 200
            }
        },
        {
            "type": "instability",
            "args": {
                "module": "/internal/graph",
                "max": 0.1
            }
        },
        {
            "type": "function",
            "args": {
                "module": "/internal",
                "max_line_count": 75
            }
        },
        {
            "type": "file_name",
            "args": {
                "module": "/internal",
                "max_length": 12,
                "regexp": "^[a-z]+$",
                "ignore": [
                    "*_test.go"
                ]
            }
        },
        {
            "type": "size",
            "args": {
                "module": "/internal",
                "max": 49.9
            }
        }
    ]
}