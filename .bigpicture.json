{
    "port": 44525,
    "ignore": [
        "web",
        "internal/browser/pyproject"
    ],
    "validators": [
        {
            "type": "no_import",
            "args": {
                "from": "/internal/config",
                "to": "*"
            }
        },
        {
            "type": "no_import",
            "args": {
                "from": "/internal/server",
                "to": "*"
            }
        },
        {
            "type": "line_count",
            "args": {
                "module": "/internal/browser",
                "max": 200
            }
        },
        {
            "type": "instability",
            "args": {
                "module": "/internal/graph",
                "max": 0.1
            }
        },
        {
            "type": "function",
            "args": {
                "module": "/internal",
                "max_line_count": 50
            }
        }
    ]
}